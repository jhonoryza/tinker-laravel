{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tinker-laravel",
  "version": "1.0.1",
  "identifier": "tinker.labkita.my.id",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Tinker Laravel",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "url": "index.html",
        "label": "main"
      },
      {
        "title": "Settings",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "url": "settings.html",
        "label": "settings",
        "create": false
      },
      {
        "title": "Artisan Command",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "url": "artisan.html",
        "label": "artisan",
        "create": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.15",
      "frameworks": [],
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    }
  }
}
